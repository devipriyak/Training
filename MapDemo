
import java.util.HashMap;
import java.util.Map;


public class MapDemo {

       public static void main(String[] args) {
        // TODO code application logic here
        Map<String,Integer> m=new HashMap<String,Integer>();//Create HashMap Object
        m.put("Ram",67);//Insert Key along with the value
        m.put("Ravi",90);
       System.out.println(m);//Print Map Object
       // System.out.println(m.putIfAbsent("Ravi", 78));
        m.putIfAbsent("Ram",97);
        m.putIfAbsent("Rahul", 111);
        System.out.println(m);
        System.out.println("Value of Key=Ram is : "+m.get("Ram"));//get the value based on the key
        System.out.println(m);
        System.out.println(m.containsKey("ram"));//Check the specified key is available or not
        System.out.println(m.containsValue(67));//Check the specified value is available or not
        System.out.println(m.replace("Ram", 68, 56));//Replace the old value of the Ram with new value
         System.out.println(m);
    }
    
}
